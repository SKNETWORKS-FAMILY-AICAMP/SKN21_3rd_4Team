# Django 설정
DJANGO_SECRET_KEY=your-secret-key-here-change-in-production
DJANGO_DEBUG=False
DJANGO_ALLOWED_HOSTS=localhost,127.0.0.1,your-ec2-ip-here

# Qdrant 설정
QDRANT_HOST=qdrant
QDRANT_PORT=6333
QDRANT_COLLECTION=learning_ai

# OpenAI API (기존 프로젝트에서 사용 중)
OPENAI_API_KEY=your-openai-api-key-here

# Tavily API (기존 프로젝트에서 사용 중)
TAVILY_API_KEY=your-tavily-api-key-here

# LangSmith (선택사항)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=your-langchain-api-key-here
LANGCHAIN_PROJECT=learning-ai-chatbot
